{
  "name": "planmate",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "figma:text": "tsx scripts/figma/extract-text.ts",
    "figma:variables": "tsx scripts/figma/extract-variables.ts",
    "figma:colors": "tsx scripts/figma/extract-colors-from-file.ts",
    "figma:variables:env": "env FIGMA_API_KEY=$(node scripts/figma/print-token.cjs) tsx scripts/figma/extract-variables.ts",
    "figma:text:env": "env FIGMA_API_KEY=$(node scripts/figma/print-token.cjs) tsx scripts/figma/extract-text.ts",
    "figma:colors:env": "env FIGMA_API_KEY=$(node scripts/figma/print-token.cjs) tsx scripts/figma/extract-colors-from-file.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2",
    "clsx": "^2.1.1",
    "react-i18next": "^14.1.2",
    "i18next": "^23.11.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.68",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "tsx": "^4.19.1"
  }
}
